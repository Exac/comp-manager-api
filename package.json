{
  "name": "comp-manager-api",
  "version": "0.0.0",
  "description": "Backend for CompManager",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run dev:windows",
    "start:windows": "npm run build&&set NODE_ENV=production&&  node src/index.js",
    "start:linux": "npm run build&&export NODE_ENV=production&& node src/index.js",
    "dev:windows": "set NODE_ENV=development&&set PGHOST=localhost&&set PGPORT=5432&&set PGDATABASE=chollima&&set PGUSER=chollima&&set PGPASSWORD=chollima&&nodemon",
    "dev:linux": "export NODE_ENV=production&&nodemon",
    "start:watch:compilejs": "tsc -p tsconfig.json",
    "build": "tsc"
  },
  "author": "Thomas McLennan <compmanagerapi@thomasmclennan.ca>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "co": "^4.6.0",
    "connect-ensure-login": "^0.1.1",
    "connect-pg-simple": "^5.0.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "html-entities": "^1.2.1",
    "mongodb": "^3.0.10",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.6",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3",
    "pg-promise": "^8.4.5",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
