    "@types/html-entities": "^1.2.16",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.5.2",
    "@types/nodemailer": "^4.6.0",
    "@types/passport": "^0.4.5",
    "@types/passport-local": "^1.0.33",
    "@types/pg": "^7.4.10",
    "@types/pg-promise": "^5.4.3",
    "@types/uuid": "^3.4.3",
    "cookie-parser": "^1.4.3",
    "nodemon": "^1.17.5",
    "tslint": "^5.10.0"
